<?xml version="1.0"?>

<rules>

    <rule>
        <id>rule_dual-read</id>
        <formula>
            <forall var="v1" in="pat_dual-read_prev">
                <not>
                    <exists var="v2" in="pat_dual-read_cur">
                        <bfunc name="dual-read">
                            <param pos="1" var="v1" field="object"/>
                            <param pos="2" var="v2" field="object"/>
                        </bfunc>
                    </exists>
                </not>
            </forall>
        </formula>
    </rule>

